# Location of BUFG Primitives 
set_property LOC BUFGCTRL_X0Y0 [get_cells single_clk_BUFG_inst]

# Location of IO Primitives which is load of clock spine
set_property LOC IOB_X0Y172 [get_cells OBUFDS_DATACLK]

# Location of clock ports
set_property LOC IOB_X0Y73 [get_ports clk_n]
set_property LOC IOB_X0Y74 [get_ports clk_p]

# Clock net "single_clk_BUFG" driven by instance "single_clk_BUFG_inst" located at site "BUFGCTRL_X0Y0"
#startgroup
create_pblock {CLKAG_single_clk_BUFG}
add_cells_to_pblock [get_pblocks  {CLKAG_single_clk_BUFG}] [get_cells -filter { PRIMITIVE_GROUP != I/O && IS_PRIMITIVE==1 && PRIMITIVE_LEVEL !=INTERNAL && NAME!=OBUFDS_DATACLK} -of_object [get_pins -filter {DIRECTION==IN} -of_objects [get_nets -hierarchical -filter {PARENT=="single_clk_BUFG"}]]]
resize_pblock [get_pblocks {CLKAG_single_clk_BUFG}] -add {CLOCKREGION_X0Y1:CLOCKREGION_X0Y1 CLOCKREGION_X0Y3:CLOCKREGION_X0Y3}
#endgroup
